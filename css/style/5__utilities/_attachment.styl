.attachment
	position relative
	background-attachment fixed
	background-repeat no-repeat
	background-size cover
	background-position center
	&:before
		content ''
		width 100%
		height 100%
		position absolute
		top 0
	&-light
		&:before
			background-color rgba(#fff,.87)
	&-dark
		&:before
			background-color rgba(#000,.7)
			
		